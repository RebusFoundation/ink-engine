{
  "name": "process-publication",
  "version": "0.1.0",
  "description": "Process documents as W3C Publications",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "tsc --checkJs --allowJs --noEmit --lib es2015 --moduleResolution node  index.js && healthier && prettier --write --plugin-search-dir=. '**/*.{js,json,css,yml,svelte}'",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run lint",
    "test": "tap tests/*.test.js",
    "snapshots": "TAP_SNAPSHOT=1 npm run test"
  },
  "engines": {
    "node": "10"
  },
  "healthier": {
    "ignore": [
      "__sapper__",
      "rollup.config.js"
    ]
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dompurify": "^2.0.7",
    "jsdom": "^15.2.1",
    "to-vfile": "^6.0.0",
    "unzipper": "^0.10.5",
    "valid-data-url": "^2.0.0",
    "vfile": "^4.0.2"
  },
  "devDependencies": {
    "healthier": "3.2.0",
    "prettier": "1.19.1",
    "tap": "14.9.2",
    "typescript": "^3.7.2"
  },
  "private": true
}
